<mvc:View
	controllerName="UI5Mob.view.Detail"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<Page
		showNavButton="{device>/isPhone}"
		navButtonPress="onNavBack"
		class="sapUiFioriObjectPage"
		title="{i18n>detailTitle}">
		<content>
			<ObjectHeader
				 
					title="{CompanyName}"
				
				
					number=""
				
				
					numberUnit=""
				>
				 
					<ObjectAttribute text="{Surname}"/>
				
				 
				<ObjectAttribute text="{GivenName}"/>
				
			</ObjectHeader>
			<IconTabBar
				select="onDetailSelect"
				id="idIconTabBar">
				<items>
					<IconTabFilter
						key=""
						text=""
						icon="sap-icon://hint">
						<!--key="Detail" -->
						<content>
							<core:Fragment fragmentName="UI5Mob.view.DetailInfoForm" type="XML" />
						</content>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>